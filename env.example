# Configuration de l'API
NEXT_PUBLIC_API_URL=http://127.0.0.1:8000/api

# Configuration de base de données (si nécessaire)
DATABASE_URL=postgresql://username:password@localhost:5432/ronovlux_db

# Configuration d'authentification (si nécessaire)
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Configuration email (si nécessaire)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Configuration de fichiers (si nécessaire)
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# Configuration Google Maps (si nécessaire)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Configuration des médias sociaux (si nécessaire)
FACEBOOK_URL=https://facebook.com/your-page
INSTAGRAM_URL=https://instagram.com/your-account
LINKEDIN_URL=https://linkedin.com/company/your-company
